{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "Statement1",
			"Effect": "Allow",
			"Action": [
			  "s3:ListBucket",
			  "s3:GetObject",
			  "s3:PutObject",
			  "s3:ReplicateObject",
			  "s3:DeleteObject",
				"s3:GetObjectAcl",
				"s3:PutObjectAcl"
			 ],
			"Resource": [
        "arn:aws:s3::your-bucket-name",
        "arn:aws:s3::your-bucket-name/*"
      ]
		}
	]
}
